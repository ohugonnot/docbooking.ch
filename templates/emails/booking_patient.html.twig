<body style="margin: 0; padding: 0; color: #333333; font-style: normal; line-height: 1.42857143; font-size: 14px; font-family: 'Open Sans', 'Open Sans'; font-weight: normal; text-align: left; background-color: #ffffff;">
	<!-- Begin wrapper table -->
	<table class="wrapper" width="100%" style="border-collapse: collapse; margin: 0 auto;">
		<tr>
			<td class="wrapper-inner" align="center" style="font-family: 'Open Sans', 'Open Sans'; vertical-align: top; padding-bottom: 30px; width: 100%;">
				<table class="main" align="center" style="border-collapse: collapse; margin: 0 auto; text-align: left; width: 600px;">
					<tr>
						<tr>
							<td class="main-content" style="font-family: 'Open Sans', 'Open Sans'; vertical-align: top; background-color: #ffffff; padding: 10px;">
								<!-- Begin Content -->
								<table style="border-collapse: collapse;">
									<tr class="email-intro">
										<td style="font-family: 'Open Sans', 'Open Sans'; vertical-align: top;">
											<p class="greeting" style="margin-top: 0; margin-bottom: 10px;">{{patient.getFirstName()}} {{patient.getLastName()}},</p>
											<p style="margin-top: 0; margin-bottom: 10px;">
											For any question, please reply to this email to speak with your doctor or his office.</a>.
											</p>
										</td>
									</tr>
									<tr class="email-summary">
										<td style="font-family: 'Open Sans', 'Open Sans'; vertical-align: top;">
											<h1 style="font-weight: 300; line-height: 1.1; font-size: 26px; margin-top: 0; margin-bottom: 5px;">Your order <span class="no-link"> #{{ "%'08.8s"|format(appointment.getId()) }}</span>
											</h1>
											<p style="margin-top: 0; margin-bottom: 10px;">Issued <span class="no-link">{{appointment.getCreateTime().format('j/m/Y')}}</span></p>
										</td>
									</tr>
									<tr class="email-information">
										<td style="font-family: 'Open Sans', 'Open Sans'; vertical-align: top;">
											<table class="order-details" style="border-collapse: collapse; width: 100%;">
												<tr>
													<td class="address-details" style="font-family: 'Open Sans', 'Open Sans'; vertical-align: top; padding: 10px 10px 10px 0; width: 50%;">
														<h3 style="font-weight: 300; line-height: 1.1; font-size: 18px; margin-bottom: 10px; margin-top: 0;">Confirmation for:</h3>
														<p style="margin-top: 0; margin-bottom: 10px;">
															{{doctor.getTitle()}} {{doctor.getFirstName()}} {{doctor.getLastName()}}<br>
															{{doctor.getAddressLine1()}}<br>
															{{doctor.getAddressLine2()}}<br>
															{{doctor.getPostalCode()}} {{doctor.getCity()}}, {{doctor.getState()}} {{doctor.getCountry()}}<br>
															T: <a href="tel:{{doctor.getPhoneNumber()}}" style="color: #1979c3; text-decoration: none;">{{doctor.getPhoneNumber()}}</a>
															@: <a href="mailto:{{doctor.getEmail()}}" style="color: #1979c3; text-decoration: none;">{{doctor.getEmail()}}</a>
														</p>
													</td>
												</tr>
												<tr>
													<td class="address-details" style="font-family: 'Open Sans', 'Open Sans'; vertical-align: top; padding: 10px 10px 10px 0; width: 50%;">
														<h3 style="font-weight: 300; line-height: 1.1; font-size: 18px; margin-bottom: 10px; margin-top: 0;">{{ "Booking confirmed for Patient:"|trans }}</h3>
														<p style="margin-top: 0; margin-bottom: 10px;">
															{{patient.getFirstName()}} {{patient.getLastName()}}<br>
															Insurance Groupe Mutuel: {{patient.getInsuranceNum()}}<br>
															{{patient.getAddress()}}<br>
															{{patient.getPostalCode()}} {{patient.getCity()}}, {{patient.getState()}} {{patient.getCountry()}}<br>
															T: <a href="tel:{{patient.getPhoneNumber()}}" style="color: #1979c3; text-decoration: none;">{{patient.getPhoneNumber()}}</a>
															@: <a href="mailto:{{patient.getEmail()}}" style="color: #1979c3; text-decoration: none;">{{patient.getEmail()}}</a>
														</p>
													</td>
												</tr>
												<tr>
													<td class="method-info" style="font-family: 'Open Sans', 'Open Sans'; vertical-align: top; padding: 10px 10px 10px 0; width: 50%;">
														<h3 style="font-weight: 300; line-height: 1.1; font-size: 18px; margin-bottom: 10px; margin-top: 0;">For your registration, you may arrive a bit in advance to your visit.</h3>
														<dl class="payment-method checkmemo" style="margin-top: 0; margin-bottom: 10px;">
															<dt class="title" style="margin-bottom: 5px; margin-top: 0; font-weight: 400;">Thank you in advance.</dt>
														</dl>
													</td>
												</tr>
											</table>
											<table class="email-items" style="width: 100%; border-collapse: collapse; border-spacing: 0; max-width: 100%; border: 1px solid #d1d1d1;">
												<thead>
													<tr>
														<th class="item-info" style="font-family: 'Open Sans', 'Open Sans'; text-align: left; vertical-align: bottom; background-color: #f2f2f2; padding: 10px;">Date & Time</th>
														<th class="item-qty" style="font-family: 'Open Sans', 'Open Sans'; vertical-align: bottom; background-color: #f2f2f2; padding: 10px; text-align: center;">Description</th>
														<th class="item-price" style="font-family: 'Open Sans', 'Open Sans'; vertical-align: bottom; background-color: #f2f2f2; padding: 10px; text-align: right;">Estimated Price</th>
													</tr>
												</thead>
												<tbody>
													<tr>
														<td class="item-info" style="font-family: 'Open Sans', 'Open Sans'; 	vertical-align: top; padding: 10px; border-top: 1px solid #d1d1d1;">
															<p class="product-name" style="margin-top: 0; font-weight: 700; 	margin-bottom: 5px;">{{appointment.getAppDate().format('j M Y')}}</p>
															<p class="sku" style="margin-top: 0; margin-bottom: 0;">{{appointment.getAppTime().format('H:i')}}</p>
														</td>
														<td class="item-qty" style="font-family: 'Open Sans', 'Open Sans'; 		vertical-align: top; padding: 10px; border-top: 1px solid #d1d1d1; text-align: center;">General Consultation</td>
														<td class="item-price" style="font-family: 'Open Sans', 'Open Sans'; vertical-align: top; padding: 10px; border-top: 1px solid #d1d1d1; text-align: right;">
															<span class="price">CHF {{appointment.getProductPrice()}}</span>
														</td>
													</tr>
												</tbody>
												<tfoot class="order-totals">
													<tr class="subtotal">
														<th colspan="2" scope="row" style="font-family: 'Open Sans', 'Open Sans'; vertical-align: top; background-color: #f2f2f2; border-top: 1px solid #d1d1d1; font-weight: 400; padding: 10px; text-align: right;">Subtotal Estimated</th>
														<td data-th="Sous-total" style="font-family: 'Open Sans', 'Open Sans'; vertical-align: top; background-color: #f2f2f2; border-top: 1px solid #d1d1d1; padding: 10px; text-align: right;">
															<span class="price" style="white-space: nowrap;">CHF {{appointment.getProductPrice()}}</span>
														</td>
													</tr>
													<tr class="grand_total">
														<th colspan="2" scope="row" style="font-family: 'Open Sans', 'Open Sans'; vertical-align: top; background-color: #f2f2f2; border-top: 0; font-weight: 400; padding: 10px; text-align: right; padding-top: 0;">
														<strong style="font-weight: 700;">Total Estimated</strong>
														</th>
														<td data-th="Montant global" style="font-family: 'Open Sans', 'Open Sans'; vertical-align: top; background-color: #f2f2f2; border-top: 0; padding: 10px; text-align: right; padding-top: 0;">
															<strong style="font-weight: 700;"><span class="price" style="white-space: nowrap;">CHF {{appointment.getProductPrice()}}</span></strong>
														</td>
													</tr>
												</tfoot>
											</table>
										</td>
									</tr>
								</table>
								<!-- End Content -->
							</td>
						</tr>
						<tr>
						<td class="footer" style="font-family: 'Open Sans', 'Open Sans'; vertical-align: top; padding: 0 10px 10px;">
							<p class="closing" style="margin-top: 0; margin-bottom: 10px;">
								Best regards,<br>
								The DocBooking Team www.DocBooking.ch <br> <img src="https://www.docbooking.ch/assets/img/logo.png"> <br>4,place de la Gare 1020 Renens-Lausanne Switzerland <br><a href="mailto:contact-for-doctors@docbooking.ch">contact-for-doctors@docbooking.ch</a>
								</p>
						</td>
					</tr>
				</table>
			</td>
		</tr>
	</table>
	<!-- End wrapper table -->
</body>
</html>