	});
	$('input[name="lang[]"]').change(function() {
		$('#filterForm').attr('action', '{{ path('app_index') }}');
	});
	$('input[name="gen[]"]').change(function() {
		$('#filterForm').attr('action', '{{ path('app_index') }}');
	});
});
