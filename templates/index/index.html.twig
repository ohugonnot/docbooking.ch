						<span class="sortby-filter">
							<select name="sort" id="dlproSort" class="select" style="min-width: 250px">
								<option>Select</option>
								<option {% if app.request.get('svsort') == 'true' %}selected="selected"{% endif %} class="sortable" data-href="/?svsort=true" title="Shortest Waiting for Visit" value="Shortest Waiting for Visit">{{ 'Shortest Waiting for Visit'|trans }}</option>
